<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>

        </tr>
    </thead>
    <tbody>
        {{#each project}}
        <tr>
            <th scope="row">1</th>
            <td>{{this.name}}</td>
            <td>{{this.description}}</td>


        </tr>
        {{/each}}
    </tbody>
</table>